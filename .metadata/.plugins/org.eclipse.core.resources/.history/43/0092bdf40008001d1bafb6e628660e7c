public class HW2 {

	public static void main(String[] args){
		// TODO Auto-generated method stub
		int c,t;
		                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
		for(c=1;c<10;c++) 
		{
			for(t=1;t<10;t++) 
			System.out.print(c+"x"+t+"="+c*t);
		}
		System.out.println("^ↀᴥↀ^");
	}
}