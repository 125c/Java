package Hello;

import java.util.Scanner;

public class Java10_5 {

	public static void main(String[] args) {
		// TODO 自動產生的方法 Stub
		int i=24;
		System.out.println("i="+i);
		i=i>>1;
		System.out.println("i右移1:"+i);
		i=i>>1;
		System.out.println("i右移2:"+i);
		i=i>>1;
		System.out.println("i右移3:"+i);
		i=i>>1;
		System.out.println("目前i是"+i);
		i=i<<4;
		System.out.println("i左移4:"+i);
	
	}
}
