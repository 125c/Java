package polymorphism;

import Hello.CommissionEmployee;

public class PolymorphismTest {

	public static void main(String[] args) {
		// TODO 自動產生的方法 Stub
		int i;
		CommissionEmployee employeeArray[] = new CommissionEmployee[4];
		employeeArray[0]= new CommissionEmployee("志郎","炭","123123",6000,0.03);
		employeeArray[1]= new CommissionEmployee("豆子","迷","123333",5000,0.1);
		employeeArray[2]= new CommissionEmployee("善意","我七","122222",4000,0.2);
		employeeArray[3]= new CommissionEmployee("沒有輸","大哥","12333",3000,0.32);
		for(i=0;i<employeeArray.length;i++) 
		{
			System.out.printf("%s%d%s :%n%n%s%n%n","第 ",i+1," 位員工資料",employeeArray[i].toString());
		}

	}

}
