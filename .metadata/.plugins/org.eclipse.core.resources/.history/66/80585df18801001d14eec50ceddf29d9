package Hello;

public class HW {

	public static void main(String[] args) 
		
		// TODO 自動產生的方法 Stub
	
		public void NNTimes(int n){        
	        String temp = "";
	        for(int i=1; i<=n; i++){
	            for(int j=1; j<=n; j++){
	                temp += j+"x"+i+"="+i*j + "\t";
	            }
	            temp += "\n";
	        }
	        System.out.println(temp);
	    }
	

}
