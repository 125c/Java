package Hello;

public class HW5HourlyEmployee {

		public int hours;
		public int wage;
		
		public int getHours() {
			return hours;
		}
		public void setHours(int hours) {
			this.hours = hours;
		}
		public int getWage() {
			return wage;
		}
		public void setWage(int wage) {
			this.wage = wage;
		}
		public HW5HourlyEmployee(int hours,int wage) 
		{
			
		}
		public double getPaymentAmount() 
		{
			return (getHours()*getWage());	
		}
}
