package Hello;

public class Java08 {

	public static void main(String[] args) {
		// TODO 自動產生的方法 Stub
		System.out.println("這會換行  >> hello");
		System.out.print("這不會換行 >> This is a book");
		System.out.print("這會接在在上個字串後面");
		int a =109;
		System.out.print("輸出整數：");
		System.out.println(a);
		float b=5492.12f;
		System.out.print("輸出單精準浮點數：");
		System.out.println(b);
		double c=5492.12f;
		System.out.print("輸出雙精準浮點數：");
		System.out.println(c);
		char d=97;
		System.out.print("輸出字元：");
		System.out.println(d);
		String e="中文";
		System.out.print("輸出字串：");
		System.out.println(e);
	}

}
